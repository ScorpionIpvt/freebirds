<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.goat" name="Greatest of all time Iptv" version="1.0.1" provider-name="Freebirds">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.addon.common" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="init.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Greatest of all time Iptv</summary>
    <website></website>
    <description lang="en">Watch premium Live tv. No kicking out. Channels from all over the world. USA, UK,and more.</description>
    <forum></forum>
    <email></email>
    <disclaimer lang="en"> This addon is regulated by General Public License:https://en.wikipedia.org/wiki/GNU_General_Public_License</disclaimer>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.freebirds" name="Freebirds Repository" version="1.0.0" provider-name="Playonmonkey">
	<extension point="xbmc.addon.repository" name="freebirds">
		<info compressed="false">https://raw.githubusercontent.com/ScorpionIpvt/freebirds/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/ScorpionIpvt/freebirds/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/ScorpionIpvt/freebirds/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Freebirds Repository</summary>
		<description>Freebirds collection of Add ons</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
